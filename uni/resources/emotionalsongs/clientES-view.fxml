<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<TabPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="604.0" prefWidth="840.0" tabClosingPolicy="UNAVAILABLE"
         xmlns="http://javafx.com/javafx/17.0.2-ea" fx:controller="emotionalsongs.clientESController">
    <Tab text="Canzoni">
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>
            <Label contentDisplay="RIGHT" graphicTextGap="16.0" prefHeight="30.0" text="Titolo"
                   AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="215.0" AnchorPane.topAnchor="0.0">
                <graphic>
                    <TextField fx:id="titleField" prefHeight="30.0"
                               prefWidth="470.0"/>
                </graphic>
            </Label>
            <Label contentDisplay="RIGHT" graphicTextGap="10.0" prefHeight="30.0" text="Autore"
                   AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="215.0" AnchorPane.topAnchor="40.0">
                <graphic>
                    <TextField fx:id="authorField" prefHeight="30.0"
                               prefWidth="470.0"/>
                </graphic>
            </Label>
            <Label contentDisplay="RIGHT" graphicTextGap="18.0" prefHeight="30.0" text="Anno"
                   AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="215.0" AnchorPane.topAnchor="80.0">
                <graphic>
                    <TextField fx:id="yearField" prefHeight="30.0"
                               prefWidth="470.0"/>
                </graphic>
            </Label>
            <Label fx:id="alertLbl" alignment="CENTER" graphicTextGap="19.0" layoutX="40.0"
                   layoutY="110.0" prefHeight="30.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0"
                   AnchorPane.topAnchor="115.0"/>
            <ChoiceBox fx:id="queryCBox" layoutX="637.0" layoutY="20.0" prefHeight="30.0" prefWidth="163.0"
                       AnchorPane.leftAnchor="600.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="0.0"/>
            <TableView fx:id="songTable" layoutX="40.0" layoutY="170.0" prefHeight="339.0" prefWidth="760.0"
                       AnchorPane.bottomAnchor="70.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0"
                       AnchorPane.topAnchor="150.0">
                <columns>
                    <TableColumn prefWidth="75.0" text="Titolo"/>
                    <TableColumn prefWidth="75.0" text="Autore"/>
                    <TableColumn prefWidth="75.0" text="Album"/>
                    <TableColumn prefWidth="75.0" text="Anno"/>
                    <TableColumn prefWidth="75.0" text="Emozione"/>
                </columns>
                <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                </columnResizePolicy>
            </TableView>
            <Button defaultButton="true" layoutX="365.0" layoutY="520.0" mnemonicParsing="false"
                    onAction="#song" prefHeight="30.0" prefWidth="0.0" text="Ricerca canzoni"
                    AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="320.0" AnchorPane.rightAnchor="320.0"/>
            <Button fx:id="addSongBtn_song" layoutX="660.0" layoutY="515.0" mnemonicParsing="false"
                    onAction="#addSong" prefHeight="30.0" prefWidth="130.0" text="Aggiungi canzone"
                    AnchorPane.bottomAnchor="35.0" AnchorPane.rightAnchor="20.0"/>
            <Button fx:id="remSongBtn_song" layoutX="660.0" layoutY="550.0" mnemonicParsing="false"
                    onAction="#remSong" prefHeight="30.0" prefWidth="130.0" text="Rimuovi canzone"
                    AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="20.0"/>
        </AnchorPane>
    </Tab>
    <Tab text="Account">
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>
            <Label fx:id="userLbl" layoutX="50.0" layoutY="40.0" prefHeight="30.0" text="Ciao"
                   AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="215.0" AnchorPane.topAnchor="0.0"/>
            <Label alignment="CENTER" graphicTextGap="19.0" layoutX="40.0" layoutY="110.0"
                   prefHeight="30.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0"
                   AnchorPane.topAnchor="35.0"/>
            <ChoiceBox fx:id="plistCBox" layoutX="637.0" layoutY="20.0" prefHeight="30.0" prefWidth="163.0"
                       AnchorPane.leftAnchor="600.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="0.0"/>
            <TableView fx:id="plistTable" layoutX="60.0" layoutY="110.0" prefHeight="372.0" prefWidth="760.0"
                       AnchorPane.bottomAnchor="70.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0"
                       AnchorPane.topAnchor="70.0">
                <columns>
                    <TableColumn prefWidth="75.0" text="Title"/>
                    <TableColumn prefWidth="75.0" text="Autore"/>
                    <TableColumn prefWidth="75.0" text="Album"/>
                    <TableColumn prefWidth="75.0" text="Anno"/>
                    <TableColumn prefWidth="75.0" text="Emozione"/>
                </columns>
                <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                </columnResizePolicy>
            </TableView>
            <Button defaultButton="true" layoutX="340.0" layoutY="550.0" mnemonicParsing="false"
                    onAction="#account" prefHeight="30.0" prefWidth="200.0" text="Accedi / Registrati"
                    AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="320.0" AnchorPane.rightAnchor="320.0"/>
            <Button fx:id="addPListBtn" layoutX="30.0" layoutY="484.0" mnemonicParsing="false"
                    onAction="#addPList" prefHeight="30.0" prefWidth="120.0" text="Crea playlist"
                    AnchorPane.bottomAnchor="35.0" AnchorPane.leftAnchor="20.0"/>
            <Button fx:id="remPListBtn" layoutX="140.0" layoutY="525.0" mnemonicParsing="false"
                    onAction="#remPList" prefHeight="30.0" prefWidth="120.0" text="Elimina playlist"
                    AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="20.0"/>
            <Button fx:id="remSongBtn_acc" layoutX="660.0" layoutY="550.0" mnemonicParsing="false"
                    onAction="#remSong" prefHeight="30.0" prefWidth="130.0" text="Rimuovi canzone"
                    AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="20.0"/>
        </AnchorPane>
    </Tab>
</TabPane>
